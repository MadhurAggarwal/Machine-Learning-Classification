   "source": [
    "x1=np.arange(-50,5)\n",
    "x2=np.arange(-5,50)\n",
    "x=np.c_[x1,x2]\n",
    "w=np.array([1,1])\n",
    "b=-3\n",
    "z=np.dot(x,w)+b\n",
    "y=1/(1+np.exp(-z))\n",
    "plt.plot(x,y)\n",
    "plt.show()"
   ]
